{
  "expo": {
    "name": "MLSA",
    "slug": "mlsa-v2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mainlogo/mainlogo.png",
    "scheme": "mlsav2",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.mlsa.app"
    },
    "android": {
      "package": "com.mlsa.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/mainlogo/mainlogo.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/illustration/Group_5.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6e5722d8-fb20-4e0a-b8da-bb61747c3776"
      }
    }
  }
}
